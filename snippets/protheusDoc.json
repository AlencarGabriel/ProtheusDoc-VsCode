{
    "TYPE": {
        "prefix": "@type",
        "body": [
            "@type ${1|function,class,method|}"
        ],
        "description": "Identifica o tipo do ProtheusDoc que está sendo documentado. "
    },
    "SINCE": {
        "prefix": "@since",
        "body": [
            "@since ${1:$CURRENT_DATE/$CURRENT_MONTH/$CURRENT_YEAR}"
        ],
        "description": "Identifica a partir de quando, uma determinada funcionalidade foi implementada. "
    },
    "PARAM": {
        "prefix": "@param",
        "body": [
            "@param ${1:param_name}, ${2|param_type,numeric,character,date,codeblock,logical,array,object,variadic|}, ${3:param_description}"
        ],
        "description": "Adiciona uma especificação de parâmetro (de função ou método), identificando-o como parameter-name."
    },
    "RETURN": {
        "prefix": "@return",
        "body": [
            "@return ${1|return_type,numeric,character,date,codeblock,logical,array,object,variadic|}, ${2:return_description}"
        ],
        "description": "Especifica o retorno (de função ou método)."
    },
    "EXAMPLE": {
        "prefix": "@example",
        "body": [
            "@example",
            "${1:(examples)}"
        ],
        "description": "Cria uma entrada no tópico 'Exemplos'."
    },
    "SEE": {
        "prefix": "@see",
        "body": [
            "@see ${1:(links_or_references)}"
        ],
        "description": "Adiciona uma entrada 'Veja também'."
    },
    "OBS": {
        "prefix": "@obs",
        "body": [
            "@obs ${1:obs-text}"
        ],
        "description": "Adiciona uma observação."
    },
    "LINK": {
        "prefix": "@link",
        "body": [
            "@link ${1:link-text}"
        ],
        "description": "Cria uma ligação (link) para o target especificado. Esta marcação deve ser utilizada como complemento nas demais marcações."
    }
}